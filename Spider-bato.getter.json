{
  "name": "Bato_Chapter_Getter",
  "description": "Fetches chapter pages (images) from mto.to safely",
  "types": {
    "fields": [
      { "name": "page_num", "type": "number" },
      { "name": "page_image", "type": "url" }
    ]
  },
  "targetUrl": "https://mto.to/chapter/{id}",
  "itemConfig": {
    "selector": null,
    "PageImage": {
      "arrayVar": {
        "variableNames": ["imgHttps"],
        "matchPattern": "https://"
      }
    }
  }
}
